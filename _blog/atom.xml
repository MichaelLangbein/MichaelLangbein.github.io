---
layout: feed
---

<?xml version="1.0" encoding="utf-8"?>

<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Code and Colors</title>
  <link href="https://michaellangbein.github.io/blog/atom.xml" rel="self" />
  <link href="https://michaellangbein.github.io/blog" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://michaellangbein.github.io/blog</id>
  <author>
    <name>Michael Langbein</name>
    <email>michael_langbein@hotmail.de</email>

    {% for post in site.posts %}

    <entry>
      <title>{{ post.title }}</title>
      <link href="https://michaellangbein.github.io{{ post.url }}" />
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>https://michaellangbein.github.io{{ post.id }}</id>
      <content type="html">{{ post.content | xml_escape  }}</content>
    </entry>

    {% endfor %}
